<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Simple markers</title>
    <style>
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
      }
      #map {
        height: 100%;
      }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script>

      function initMap() {
        var theirLatLng = {lat: -25.363, lng: 131.044};

        var LatLng = {lat: 49.24, lng: -123.11};

        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 8,
          center: LatLng
        });

        var thiermarker = new google.maps.Marker({
          position: theirLatLng,
          map: map,
          draggable: true,
          animation: google.maps.Animation.DROP,
          title: 'Hello World!'
        });

        var myMarker = new google.maps.Marker({
          position: LatLng,
          map: map,
          draggable: true,
          animation: google.maps.Animation.DROP,
          title: 'My Marker.'
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCfSrmdKzAuWFZ3EFdELQ786dkTus7RYHo&callback=initMap">
    </script>
  </body>
</html>